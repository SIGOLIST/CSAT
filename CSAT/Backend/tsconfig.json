{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": false,
    "baseUrl": "App",
    "paths": {
        "Model/*" : [ "Model/*" ],
        "Router/*" : [ "Router/* "],
        "Service/*" : [ "Service/* "],
        "API/*" : [ "Service/HttpService/* "]
    }
  },
  "exclude": [
    "typings",
    "node_modules"
    ]
}
